	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-action>#{userProfileBean.login}</from-action>
			<from-outcome>blocked</from-outcome>
			<to-view-id>/pages/user/Blocked.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{userProfileBean.login}</from-action>
			<from-outcome>login-failed</from-outcome>
			<to-view-id>/pages/user/LoginFailed.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{manageClipEditBean.init}</from-action>
			<from-outcome>manageClipEditBean-init-complete</from-outcome>
			<to-view-id>/pages/admin/EditClip.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{manageClipEditBean.update}</from-action>
			<from-outcome>return-manage-clip</from-outcome>
			<to-view-id>/pages/admin/ManageClip.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adminAuthFailure</from-outcome>
			<to-view-id>/pages/client/Zentai.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>adminHomePage</from-outcome>
			<to-view-id>/pages/admin/HomePage.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{manageSingerEditBean.init}</from-action>
			<from-outcome>manageSingerEditBean-init-complete</from-outcome>
			<to-view-id>/pages/admin/EditSinger.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{manageSingerEditBean.update}</from-action>
			<from-outcome>return-manage-singer</from-outcome>
			<to-view-id>/pages/admin/ManageSinger.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adminManageClip</from-outcome>
			<to-view-id>/pages/admin/ManageClip.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adminManageSinger</from-outcome>
			<to-view-id>/pages/admin/ManageSinger.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
