		</navigation-case>
		<navigation-case>
			<from-outcome>adminManageGenre</from-outcome>
			<to-view-id>/pages/admin/ManageGenre.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adminManageClipUpload</from-outcome>
			<to-view-id>/pages/admin/UploadClip.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>manageComposerEditBean-init-complete</from-outcome>
			<to-view-id>/pages/admin/EditComposer.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{manageComposerEditBean.update}</from-action>
			<from-outcome>return-manage-composer</from-outcome>
